local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("üêí Moneky Hub ", "DarkTheme")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("AutoFarm")

MainSection:NewToggle("AutoJump", "AutoJumps for xp to level up", function(state)
    if state then
        getgenv().toggle = true
while task.wait() do
    if toggle == true then
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    else
        break
    end
end
    else
        getgenv().toggle = false
while task.wait() do
    if toggle == true then
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    else
        break
    end
end
    end
end)
MainSection:NewToggle("Gummy AutoFarm", "Auto collect gummydrops", function(state)
    if state then
        getgenv().toggle1 = true
while task.wait() do
    if getgenv().toggle1 then
        for i, v in pairs(game:GetService("Workspace").Floor:GetDescendants()) do
            if v.Name == "GumdropHitbox" and v:IsA("Part") then
                game.Players.LocalPlayer.Character.Bean.CFrame = v.CFrame
            end
        end
    end
end
    else
        getgenv().toggle1 = false
while task.wait() do
    if getgenv().toggle1 then
        for i, v in pairs(game:GetService("Workspace").Floor:GetDescendants()) do
            if v.Name == "GumdropHitbox" and v:IsA("Part") then
                game.Players.LocalPlayer.Character.Bean.CFrame = v.CFrame
            end
        end
    end
end
    end
end)
MainSection:NewToggle("Gravity", "Gravity", function(state)
    if state then
        Workspace.Gravity = 1000
    else
        Workspace.Gravity = 150
    end
end)
MainSection:NewButton("AntiAfk", "Makes it so you dont get kicked for being afk", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/sDXcYFhR", true))()
end)
local Tab = Window:NewTab("Scripts")
local Settings = Window:NewTab("Settings")
local SettingsSection = Settings:NewSection("Settings")
SettingsSection:NewKeybind("Gui toggle", "Toggle the gui on and off", Enum.KeyCode.V, function()
    Library:ToggleUI()
end)
local ScriptsSection = Tab:NewSection("Scripts")
ScriptsSection:NewButton("Inf Yeild", "Loads infiniteyield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
ScriptsSection:NewButton("Vapev4", "Loads Vapev4", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
end)
MainSection:NewButton("AutoAbility", "Does all 3 of your abilitys using a autoclicker is reccomended", function()
    game:GetService("ReplicatedStorage").RemoteFunctions.UseAbility:InvokeServer("Spin",true)

game:GetService("ReplicatedStorage").RemoteFunctions.UseAbility:InvokeServer("Dash",true)

game:GetService("ReplicatedStorage").RemoteFunctions.UseAbility:InvokeServer("Launch",true)
end)

MainSection:NewButton("AutoBounce", "Sends packets that you have bounced giving you xp", function()
    while wait() do
   -- Script generated by TurtleSpy, made by Intrer#0421

game:GetService("ReplicatedStorage").Events.OnLandFromJump:FireServer()
end
end)
MainSection:NewButton("AutoTrampoline", "Sends packets that you are on a trampoline giving you xp", function()
    while wait() do
   game:GetService("ReplicatedStorage").Events.InteractObject:FireServer(workspace.Gimmicks.AngledSpring.Model.BouncePad.BouncePad)
end
end)
MainSection:NewButton("AutoRing", "Gives you xp from sending ring packets", function()
    getgenv().toggle = true
while task.wait() do
    if getgenv().toggle then
        game:GetService("ReplicatedStorage").Events.InteractObject:FireServer(workspace.Gimmicks.WarpTrickRing.TrickRingHitbox)
    else
        break
    end
end
end)
MainSection:NewButton("AutoBumper", "ButtonInfo", function()
        while wait() do
   game:GetService("ReplicatedStorage").Events.InteractObject:FireServer(workspace.Gimmicks.Bumper)
end
end)
MainSection:NewToggle("ToggleText", "ToggleInfo", function(state)
    if state then
        getgenv().toggle = true
while task.wait() do
    if getgenv().toggle then
        while wait() do
   game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("monkey hub on top üêíüêíüêíüêíüêí ","All") 
end
    else
        break
    end
end
    else
        getgenv().toggle = false
while task.wait() do
    if getgenv().toggle then 
        while wait() do 
   game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("monkey hub on top üêíüêíüêíüêíüêí ","All") 
end
    else
        break
    end
end
    end
end)
